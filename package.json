{
  "name": "waste-export-controls-tool",
  "version": "0.0.1",
  "description": "This tool helps you to identify the appropriate classification for waste which is intended for export and the regulatory controls which apply to exports of those wastes to certain countries.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build:css": "bin/build-css",
    "build": "bin/build",
    "lint": "standard",
    "unit-test": "lab -c -r console -o stdout -r lcov -o lcov.info",
    "test": "npm run lint && npm run unit-test",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@defra/hapi-gapi": "^1.0.1",
    "@hapi/boom": "^9.1.0",
    "@hapi/good": "^9.0.0",
    "@hapi/good-console": "^9.0.0",
    "@hapi/good-squeeze": "^6.0.0",
    "@hapi/hapi": "^19.2.0",
    "@hapi/inert": "^6.0.1",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^6.0.0",
    "@hapi/yar": "^10.1.0",
    "accessible-autocomplete": "^2.0.3",
    "blipp": "^4.0.1",
    "govuk-frontend": "^3.7.0",
    "nunjucks": "^3.2.2"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.1",
    "@hapi/lab": "^22.0.4",
    "codeclimate-test-reporter": "^0.5.1",
    "node-sass": "^4.14.1",
    "standard": "^14.3.4"
  }
